{
  "name": "@trustgraph/react-provider",
  "version": "1.1.0",
  "description": "React provider for TrustGraph WebSocket connections",
  "type": "module",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prettify": "prettier --write --print-width 79 .",
    "prepare": "npm run build"
  },
  "keywords": [
    "trustgraph",
    "react",
    "websocket",
    "provider"
  ],
  "author": "KnowNext Limited",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@trustgraph/client": "^1.1.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@trustgraph/client": "^1.1.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.3.1",
    "rollup": "^4.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.0",
    "vitest": "^3.2.4"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trustgraph-ai/trustgraph-react-provider.git"
  },
  "bugs": {
    "url": "https://github.com/trustgraph-ai/trustgraph-react-provider/issues"
  },
  "homepage": "https://github.com/trustgraph-ai/trustgraph-react-provider#readme"
}
